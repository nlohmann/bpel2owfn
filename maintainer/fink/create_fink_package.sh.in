#/!bin/bash

sudo cp bpel2owfn.info /sw/fink/dists/local/main/finkinfo/

fink validate bpel2owfn.info

fink -m --build-as-nobody rebuild bpel2owfn
dpkg -c /sw/fink/debs/bpel2owfn_@PACKAGE_VERSION@-1_darwin-i386.deb

cp /sw/fink/dists/local/main/binary-darwin-i386/bpel2owfn_@PACKAGE_VERSION@-1_darwin-i386.deb .

fink install bpel2owfn
fink remove bpel2owfn

sudo rm /sw/fink/dists/local/main/finkinfo/bpel2owfn.info
sudo rm /sw/src/bpel2owfn-@PACKAGE_VERSION@.tar.gz
sudo rm /sw/fink/dists/local/main/binary-darwin-i386/bpel2owfn_@PACKAGE_VERSION@-1_darwin-i386.deb
