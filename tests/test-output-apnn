#!/usr/bin/env bash

command="bpel2owfn --input=$testdir/testfiles/purchaseOrderProcess.bpel --mode=petrinet --format=apnn --output=purchaseOrderProcess"
echo ""
echo "test: Petri net file output: APNN format"
echo "      $command"
$command &> /dev/null

if [ $? -eq 0 ]
	then rm $builddir/purchaseOrderProcess.apnn
fi
